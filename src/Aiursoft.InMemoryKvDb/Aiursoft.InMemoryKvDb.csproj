<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>10.0.2</Version>
    <TargetFramework>net10.0</TargetFramework>
    <AssemblyName>Aiursoft.InMemoryKvDb</AssemblyName>
    <RootNamespace>Aiursoft.InMemoryKvDb</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Aiursoft</Company>
    <Product>InMemoryKvDb</Product>
    <Description>Nuget package of 'InMemoryKvDb' provided by Aiursoft</Description>
    <PackageId>Aiursoft.InMemoryKvDb</PackageId>
    <PackageTags>nuget package dotnet csproj dependencies</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://gitlab.aiursoft.com/aiursoft/inmemorykvdb</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://gitlab.aiursoft.com/aiursoft/inmemorykvdb.git</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.2" />
    <None Include="../../README.md" Pack="true" PackagePath="." />
  </ItemGroup>
</Project>